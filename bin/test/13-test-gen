#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

ids=(
"0x2a0500010500010200000000000000050000000000000000000000000000019b"
"0x2a0500010500010200000000000000050000000000000000000000000000019c"
"0x2a0500010500010200000000000000050000000000000000000000000000019e"
"0x2a0500010500010200000000000000050000000000000000000000000000019d"
"0x2a0500010500010200000000000000050000000000000000000000000000019f"
"0x2a050001050001020000000000000005000000000000000000000000000001a0"
  )

for (( i=0; i<${#ids[@]}; i++ ));
do
  id="${ids[$i]}"
  evo sts apo $id --chain polygon
done
