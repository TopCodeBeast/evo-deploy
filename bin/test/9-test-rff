#!/usr/bin/env bash

set -e

ids=(
"0x2a040001040001010000000000000004000000000000000000000000000006a5"
"0x2a04000104000101000000000000000400000000000000000000000000000616"
"0x2a04000104000101000000000000000400000000000000000000000000000319"
"0x2a040001040001010000000000000004000000000000000000000000000005dc"
"0x2a040001040001010000000000000004000000000000000000000000000004fb"
"0x2a0400010400010100000000000000040000000000000000000000000000076e"
"0x2a040001040001010000000000000004000000000000000000000000000006f3"
"0x2a04000104000101000000000000000400000000000000000000000000000768"
"0x2a04000104000101000000000000000400000000000000000000000000000539"
"0x2a040001040001010000000000000004000000000000000000000000000001dc"
"0x2a04000104000101000000000000000400000000000000000000000000000166"
  )
for id in "${ids[@]}"; do
  echo $id
  evo mkt-gen claim $id --chain heco
done

