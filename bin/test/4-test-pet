#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

owner=0x0f14341A7f464320319025540E8Fe48Ad0fe5aec
to=0xa1511E5C683A007056CAa1d9A8D6A37464826280
gene=39

for i in {1..2}; do
  seth send "$CK" "createPromoKitty(uint256,address)" "$(($gene+$i))" "$to"
done

