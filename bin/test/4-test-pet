#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

owner=0x0f14341A7f464320319025540E8Fe48Ad0fe5aec
to=0xf422673CB7a673f595852f7B00906408A0b073db
gene=39

for i in {1..2}; do
  seth send "$CK" "createPromoKitty(uint256,address)" "$(($gene+$i))" "$owner"
done

for i in {1..2}; do
  seth send -F "$owner" "$CK" "transfer(address,uint256)" "$to" "$(($gene+$i))" 
done
