#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

apo0=0x2a0400010400010200000000000000040000000000000000000000000000003c
apo1=0x2a0400010400010200000000000000040000000000000000000000000000020c
apo2=0x2a0400010400010200000000000000040000000000000000000000000000003b

land=0x2a04000104000101000000000000000400000000000000000000000000000272
# seth send $PVETEAM_PROXY "join(uint256,uint256)" 0 "$apo1"
seth send $PVETEAM_PROXY "join(uint256,uint256)" 0 "$apo0"
seth send $PVETEAM_PROXY "join(uint256,uint256)" 1 "$apo2"
# seth send $LANDRESOURCE_PROXY "startMining(uint256,uint256,address)" $apo0 $land $TOKEN_ERC20_GOLD
seth send $PVETEAM_PROXY "exit(uint256)" "$apo0"
# seth send $PVETEAM_PROXY "evict(uint256)" "$apo0"
