#!/usr/bin/env bash

. $(PWD)/bin/init

cd "lib/common-contracts"

auths=(
$OBJECTOWNERSHIP_AUTHORITY
$DRILLBASE_AUTHORITY
$TOKENLOCATION_AUTHORITY
$TOKENUSE_AUTHORITY
$MINTANDBURN_AUTHORITY
$APOSTLECLOCKAUCTION_AUTHORITY
$APOSTLEBASE_AUTHORITY
$USERPOINTS_AUTHORITY
$LANDBASE_AUTHORITY
$LANDCLOCKAUCTION_AUTHORITY
$MYSTERIOUSTREASURE_AUTHORITY
$LANDRESOURCE_AUTHORITY
$PVEENTRY_AUTHORITY
$PVEMATERIAL_AUTHORITY
)
for auth in "${auths[@]}"
do
  dapp verify-contract-subscan contracts/Authority.sol:Authority $auth
done

cd -
