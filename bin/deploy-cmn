#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

settings=$(dapp create SettingsRegistry)
cat $ADDRESSES_FILE | jq --arg key "SETTINGSREGISTRY" --arg value "$settings" '.[$key] = $value' | sponge $ADDRESSES_FILE

interstellar=$(dapp create InterstellarEncoderV3)
cat $ADDRESSES_FILE | jq --arg key "INTERSTELLARENCODERV3" --arg value "$interstellar" '.[$key] = $value' | sponge $ADDRESSES_FILE
