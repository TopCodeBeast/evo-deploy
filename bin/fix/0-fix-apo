#!/usr/bin/env bash

set -e

p() { printf "%-66s %-66s\n" "$1" "$2"; }

ids=(
"0x2a030001030001020000000000000003000000000000000000000000000000a1"
"0x2a030001030001020000000000000003000000000000000000000000000000a2"
"0x2a030001030001020000000000000003000000000000000000000000000000a3"
"0x2a030001030001020000000000000003000000000000000000000000000000a4"
"0x2a030001030001020000000000000003000000000000000000000000000000a5"
"0x2a030001030001020000000000000003000000000000000000000000000000a6"
"0x2a030001030001020000000000000003000000000000000000000000000000a7"
"0x2a030001030001020000000000000003000000000000000000000000000000a8"
"0x2a030001030001020000000000000003000000000000000000000000000000a9"
"0x2a030001030001020000000000000003000000000000000000000000000000aa"
"0x2a030001030001020000000000000003000000000000000000000000000000ab"
"0x2a030001030001020000000000000003000000000000000000000000000000ac"
"0x2a030001030001020000000000000003000000000000000000000000000000ad"
"0x2a030001030001020000000000000003000000000000000000000000000000ae"
"0x2a030001030001020000000000000003000000000000000000000000000000af"
"0x2a030001030001020000000000000003000000000000000000000000000000b0"
"0x2a030001030001020000000000000003000000000000000000000000000000b1"
"0x2a030001030001020000000000000003000000000000000000000000000000b2"
"0x2a030001030001020000000000000003000000000000000000000000000000b3"
"0x2a030001030001020000000000000003000000000000000000000000000000b4"
"0x2a030001030001020000000000000003000000000000000000000000000000b5"
"0x2a030001030001020000000000000003000000000000000000000000000000b6"
"0x2a030001030001020000000000000003000000000000000000000000000000b7"
"0x2a030001030001020000000000000003000000000000000000000000000000b8"
"0x2a030001030001020000000000000003000000000000000000000000000000b9"
"0x2a030001030001020000000000000003000000000000000000000000000000ba"
"0x2a030001030001020000000000000003000000000000000000000000000000bb"
"0x2a030001030001020000000000000003000000000000000000000000000000bc"
"0x2a030001030001020000000000000003000000000000000000000000000000bd"
"0x2a030001030001020000000000000003000000000000000000000000000000be"
)

chain=crabtest
for (( i=0; i<${#ids[@]}; i++ ));
do
  id="${ids[$i]}"
  owner=$(evo cmn-tkn owner $id --chain $chain)
  p "$id" "$owner"
  evo apo-auc cancel $id --chain $chain
done

