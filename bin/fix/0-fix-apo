#!/usr/bin/env bash

set -e

p() { printf "%-66s %-66s\n" "$1" "$2"; }

ids=(

"0x2a03000103000102000000000000000300000000000000000000000000000005"
"0x2a03000103000102000000000000000300000000000000000000000000000006"
"0x2a03000103000102000000000000000300000000000000000000000000000007"
"0x2a03000103000102000000000000000300000000000000000000000000000008"
"0x2a03000103000102000000000000000300000000000000000000000000000009"
"0x2a0300010300010200000000000000030000000000000000000000000000000a"
"0x2a0300010300010200000000000000030000000000000000000000000000000b"
"0x2a0300010300010200000000000000030000000000000000000000000000000d"
"0x2a0300010300010200000000000000030000000000000000000000000000000c"
"0x2a0300010300010200000000000000030000000000000000000000000000000e"
"0x2a0300010300010200000000000000030000000000000000000000000000000f"
"0x2a03000103000102000000000000000300000000000000000000000000000010"
"0x2a03000103000102000000000000000300000000000000000000000000000011"
"0x2a03000103000102000000000000000300000000000000000000000000000012"
"0x2a03000103000102000000000000000300000000000000000000000000000013"
"0x2a03000103000102000000000000000300000000000000000000000000000014"
"0x2a03000103000102000000000000000300000000000000000000000000000015"
"0x2a03000103000102000000000000000300000000000000000000000000000016"
)

chain=crab
for (( i=0; i<${#ids[@]}; i++ ));
do
  id="${ids[$i]}"
  owner=$(evo cmn-tkn owner $id --chain $chain)
  p "$id" "$owner"
  evo apo-auc cancel $id --chain $chain
done

