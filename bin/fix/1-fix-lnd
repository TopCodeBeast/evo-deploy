chain=crabtest
for (( i=0; i<${#ids[@]}; i++ ));
do
  id="${ids[$i]}"
  owner=$(evo cmn-tkn owner $id --chain $chain)
  p "$id" "$owner"
  evo mkt-auc cancel $id --chain $chain
done

